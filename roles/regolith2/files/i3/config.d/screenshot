bindsym Print exec --no-startup-id maim "/home/$USER/Images/shots/$(date +"%m-%d-%Y-%s").jpg"
bindsym $mod+Print exec --no-startup-id maim --window $(xdotool getactivewindow) "/home/$USER/Images/shots/$(date +"%m-%d-%Y-%s").jpg"
bindsym Shift+Print exec --no-startup-id maim --select "/home/$USER/Images/shots/$(date +"%m-%d-%Y-%s").jpg"
